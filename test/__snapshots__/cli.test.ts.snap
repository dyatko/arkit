scale max 1920 width
left to right direction

skinparam nodesep 16
skinparam ranksep 32
skinparam monochrome true
skinparam shadowing false
skinparam defaultFontName Tahoma
skinparam defaultFontSize 12
skinparam roundCorner 6
skinparam dpi 150
skinparam arrowColor black
skinparam arrowThickness 0.5
skinparam packageTitleAlignment left

skinparam usecase {
  borderThickness 0.5
}

skinparam rectangle {
  borderThickness 0.5
}

skinparam component {
  borderThickness 1
}


(<b>ReactDOMUnstableNativeDependencies</b>)
[<b>shared</b>] as _shared

package "Events" {
  (<b>DOMEventPluginOrder</b>)
  (<b>EventListener</b>)
  (<b>FallbackCompositionState</b>)
  (<b>getEventCharCode</b>)
  (<b>getEventModifierState</b>)
  (<b>getVendorPrefixedEventName</b>)
  (<b>isEventSupported</b>)
  (<b>SyntheticAnimationEvent</b>)
  (<b>SyntheticClipboardEvent</b>)
  (<b>SyntheticCompositionEvent</b>)
  (<b>SyntheticInputEvent</b>)
  (<b>SyntheticTransitionEvent</b>)
  (<b>SyntheticUIEvent</b>)
}

package "Client" {
  (<b>getActiveElement</b>)
  (<b>inputValueTracking</b>)
  (<b>ReactDOMComponentTree</b>)
  (<b>ToStringValue</b>)
  (<b>validateDOMNesting</b>)
}

package "Server" {
  (<b>escapeTextForBrowser</b>)
  (<b>ReactDOMFizzServerBrowser</b>)
  (<b>ReactDOMFizzServerFormatConfig</b>)
  rectangle "<b>ReactThreadIDAllocator</b>" as _ReactThreadIDAllocator
}


